{
  "name": "codeinc/mailer-email-to-mime-message",
  "version": "1.0.0",
  "description": "A library to convert Code Inc.’s mailer emails to MIME messages using a component of the Zend Framework",
  "homepage": "https://github.com/CodeIncHQ/MailerEmailToMimeMessage",
  "type": "library",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "zendframework/zend-mail": "^2.10",
    "codeinc/mailer": "^2.0"
  },
  "require-dev": {
    "codeinc/error-renderer": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "CodeInc\\MailerEmailToMimeMessage\\": "src"
    }
  },
  "authors": [
    {
      "name": "Joan Fabrégat",
      "email": "joan@codeinc.fr",
      "homepage": "https://www.codeinc.fr",
      "role": "developer"
    }
  ],
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  }
}
